<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".teacherwindow"
    android:fitsSystemWindows="true"
    android:background="@drawable/animation5tw">

    <Button
        android:id="@+id/teacher_register"
        android:layout_width="169dp"
        android:layout_height="68dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="206dp"
        android:backgroundTint="@color/dark_purple"
        android:text="REGISTER"
        android:textColor="@color/lime"
        android:textSize="20sp"
        app:icon="@drawable/add_24dp_5f6368_fill0_wght400_grad0_opsz24"
        app:iconTint="@color/lime"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teacher_login"
        app:rippleColor="@color/lime"
        app:strokeColor="@color/lime"
        app:strokeWidth="5dp" />

    <Button
        android:id="@+id/teacher_login"
        android:layout_width="158dp"
        android:layout_height="71dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="127dp"
        android:backgroundTint="@color/dark_purple"
        android:text="LOGIN"
        android:textColor="@color/lime"
        android:textSize="20sp"
        app:icon="@drawable/login_24dp_5f6368_fill0_wght400_grad0_opsz24"
        app:iconTint="@color/lime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:rippleColor="@color/lime"
        app:strokeColor="@color/lime"
        app:strokeWidth="5dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="91dp"
        android:fontFamily="@font/monoid"
        android:text="TEACHER SECTION"
        android:textColor="@color/lime"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/teacher_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>